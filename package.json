{
  "name": "activity-checker",
  "version": "1.0.0",
  "description": "User activity checker that works across browser tabs/windows.",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:dist & npm run build:prod",
    "build:dist": "webpack --progress",
    "build:dist:watch": "webpack --progress -w",
    "build:prod": "cross-env NODE_ENV=production webpack --progress",
    "test": "cross-env NODE_ENV=testing karma start",
    "test:coverage": "cross-env NODE_ENV=coverage karma start",
    "test:watch": "cross-env NODE_ENV=testing karma start --no-single-run --auto-watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Den-dp/activity-checker.git"
  },
  "keywords": [],
  "author": "Denis Bendrikov <Denis.Bendrikov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Den-dp/activity-checker/issues"
  },
  "homepage": "https://github.com/Den-dp/activity-checker#readme",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-loader": "^5.4.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "cross-env": "^2.0.0",
    "isparta": "^4.0.0",
    "isparta-loader": "^1.0.0",
    "istanbul": "^0.4.2",
    "jasmine": "^2.4.1",
    "karma": "^1.1.1",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-nyan-reporter": "^0.2.4",
    "karma-sinon": "^1.0.5",
    "karma-sinon-chai": "^1.2.3",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "rimraf": "^2.5.3",
    "rx": "^4.1.0",
    "rx-lite": "^4.0.8",
    "rxjs": "^5.0.0-beta.10",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.13.1"
  },
